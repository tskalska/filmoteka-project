e":48,"column":11}},{"name":"removeFromLocalStorage","generated":{"line":62,"column":12},"source":"js/components/to-trash-btn.js","original":{"line":48,"column":12}},{"generated":{"line":62,"column":34},"source":"js/components/to-trash-btn.js","original":{"line":48,"column":4}},{"name":"libraryType","generated":{"line":62,"column":35},"source":"js/components/to-trash-btn.js","original":{"line":48,"column":35}},{"name":"QUEUE","generated":{"line":62,"column":56},"source":"js/components/to-trash-btn.js","original":{"line":48,"column":47}},{"generated":{"line":62,"column":61},"source":"js/components/to-trash-btn.js","original":{"line":48,"column":4}},{"generated":{"line":63,"column":0},"source":"js/components/to-trash-btn.js","original":{"line":49,"column":4}},{"generated":{"line":64,"column":0},"source":"js/components/to-trash-btn.js","original":{"line":50,"column":3}},{"generated":{"line":65,"column":0},"source":"js/components/to-trash-btn.js","original":{"line":51,"column":1}}],"sources":{"js/components/to-trash-btn.js":"import btnToEmpty from './../../templates/clear-library-button.hbs';\nimport { onClickWatchedBtn, onClickQueueBtn } from './functionality-watched-queue-button';\nimport * as localBD from './localDB';\nimport libraryType from './library-type';\nimport { Confirm } from 'notiflix';\n// import Notiflix from 'notiflix';\n\nconst pageContainerEl = document.querySelector('.page-container__body');\n\nfunction renderBtnToClear() {\n  const isVisible =\n    (pageState.isWatched && localBD.getItemsFromWatched()?.length > 0) ||\n    (pageState.isQueue && localBD.getItemsFromQueue()?.length > 0);\n\n  if (isVisible) {\n    document.querySelector('.button_clear-library').classList.remove('hidden');\n    document.querySelector('.tooltip').classList.remove('hidden');\n  } else {\n    document.querySelector('.button_clear-library').classList.add('hidden');\n    document.querySelector('.tooltip').classList.add('hidden');\n  }\n}\n\nfunction initBtnToClear() {\n  pageContainerEl.insertAdjacentHTML('beforeend', btnToEmpty());\n  const buttonClearLibrary = document.querySelector('.button_clear-library');\n  buttonClearLibrary.addEventListener('click', () => {\n    Confirm.show(\n      'Delete library list', \n      'Are you sure?', \n      'Yes', \n      'No', \n      function () {\n        clearLibrary(pageState)\n      }, \n      null,\n      { titleColor:\"#ff6b01\", okButtonBackground:\"#ff6b01\"}\n    )\n  })\n}\n\nfunction clearLibrary() {\n  if (pageState.isWatched) {\n    localBD.removeFromLocalStorage(libraryType.WATCHED);\n    onClickWatchedBtn();\n  }\n  if (pageState.isQueue) {\n    localBD.removeFromLocalStorage(libraryType.QUEUE);\n    onClickQueueBtn();\n  }\n}\n\nexport { renderBtnToClear, initBtnToClear };\n"},"lineCount":null}},"error":null,"hash":"ec22b8404aa200a559e87e5627857903","cacheData":{"env":{}}}